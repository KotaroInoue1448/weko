{
  "settings": {
    "analysis": {
      "tokenizer": {
        "ngram_tokenizer": {
          "type": "ngram",
          "min_gram": 1,
          "max_gram": 2
        }
      }
    }
  },
  "mappings": {
    "numeric_detection": false,
    "dynamic": false,
    "dynamic_templates": [
      {
        "date_fields": {
          "mapping": {
            "format": "strict_date_hour_minute_second",
            "type": "date"
          },
          "match_mapping_type": "date"
        }
      }
    ],
    "date_detection": false,
    "properties": {
      "country": { "type": "keyword" },
      "is_restricted": { "type": "boolean" },
      "visitor_id": { "type": "keyword" },
      "unique_session_id": { "type": "keyword" },
      "is_robot": { "type": "boolean" },
      "site_license_name": { "type": "keyword" },
      "bucket_id": { "type": "keyword" },
      "user_group_list": {
        "type": "nested",
        "properties": {
          "group_id": { "type": "keyword" },
          "group_name": { "type": "keyword" }
        }
      },
      "hostname": { "type": "keyword" },
      "cur_user_id": { "type": "keyword" },
      "root_file_id": { "type": "keyword" },
      "item_title": { "type": "keyword" },
      "timestamp": {
        "format": "strict_date_hour_minute_second",
        "type": "date"
      },
      "accessrole": { "type": "keyword" },
      "remote_addr": { "type": "keyword" },
      "unique_id": { "type": "keyword" },
      "site_license_flag": { "type": "boolean" },
      "item_id": { "type": "keyword" },
      "billing_file_price": { "type": "keyword" },
      "userrole": { "type": "keyword" },
      "collection": { "type": "keyword" },
      "is_billing_item": { "type": "boolean" },
      "index_list": { "type": "keyword" },
      "file_key": { "type": "keyword" },
      "referrer": { "type": "keyword" },
      "size": { "type": "double" },
      "file_id": { "type": "keyword" }
    }
  }
}
